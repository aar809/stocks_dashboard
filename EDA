  {
   "cell_type": "code",
   "execution_count": 64,
   "metadata": {},
   "outputs": [],
   "source": [
    "df[\"Volatility\"] = df[\"Close\"] - df[\"Open\"]\n",
    "df[\"Change\"] = df[\"Close\"] - df[\"Close\"].shift(1)\n",
    "df[\"PercentChange\"] = (df[\"Close\"] - df[\"Close\"].shift(1))/df[\"Close\"].shift(1)*100"
   ]
  },
